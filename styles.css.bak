body{
  background-image: url("backgroundpic.png");
  font-family: Tahoma, sans-serif;
  
}
header{
  position: relative;
  top: 0;
  left: 0;
  width: 100%;
  height: 11%;
  background-color: #007BBF;
}


h1{
    text-align: center;
}

header img{
  position: relative;
  top: 0px;
  left: 13px;
}

#homepage {  
  float: right;
  right: 20px;
  padding: 5px;
  height: 75%;
  width: 15%;
  background-color: #007BBF;
  border-style: solid;
  border-width: 5px;
  border-color: white;
  color:white;
}
.maindiv{
  width: 83%;
  margin: auto;
  margin-top: 50px;
  display: flex;
}

.scnddiv{
  margin: 0 auto;
  border-style: solid;
  border-width: 1px;
  border-radius: 8px;
  width: 46%;
  float: left;
  position: relative;
  box-shadow: -10px 10px 5px gray;
  background-color: #F0F0F0;
  padding-bottom:5%;
  padding-top: 2%;
  padding-left: 1.5%;
}
label {
  display: inline-block;
  width: 20%;
  text-align: left;
  font-family: Tahoma, sans-serif;
  padding: 10px;

}
select, button{
  height: 20px;
  border-radius: 8px;
  border-width: 2px;
  border-color: #A0A0A0;
  border-style: solid;
}
input{
  height: 20px;
  border-radius: 8px;
  border-width: 2px;
  border-color: #A0A0A0;
  border-style: solid;
  min-height: 25px;
  min-width: 60%;
}
.downb{
  height: 20px;
  border-radius: 8px;
  color: white;
  background-color: #007BBF;
}


.alignpls{
    width: 60%;
    min-height: 25px;
}
.lblalign{
    display: block;
    min-height: 25px;
}
#req{
  min-width: 100%;
  max-height: 20%;
}
